<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote  name="cros"
           fetch="https://chromium.googlesource.com"
           review="https://chromium-review.googlesource.com" />
  <remote  name="aosp"
           fetch="https://android.googlesource.com"
           review="https://android-review.googlesource.com" />
  <remote  name="freedesktop"
           fetch="https://github.com/freedesktop" />
  <remote  name="eproxy"
           fetch="https://github.com/anholt" />
  <remote name="github" fetch="https://github.com/kalyankondapally" />
  <remote name="khronous" fetch="https://github.com/KhronosGroup" />
  <remote name="llvm" fetch="https://github.com/llvm" />
  <remote name="intel-github" fetch="https://github.com/intel" />
  <default revision="refs/heads/master" remote="cros" sync-j="8" />

  <include name="include/drivers.xml" />
  <include name="include/libraries_wayland.xml" />
  <include name="include/libraries_vm.xml" />
  <include name="include/libraries_x11.xml" />
  <include name="include/demos.xml" />
  
</manifest>
